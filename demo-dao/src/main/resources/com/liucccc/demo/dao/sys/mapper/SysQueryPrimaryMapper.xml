<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liucccc.demo.dao.sys.mapper.SysQueryPrimaryMapper">

    <!-- 通用查询映射结果 -->
