        <result column="groupby" property="groupby" />
        <result column="show_cols_code" property="showColsCode" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        query_id, query_name, fixe_cols, show_cols, sta_cols, tab_lst, tab_rel, condition, orderby, groupby, show_cols_code
